#include<iostream>
using namespace std;
int main()
{
cout<<"\n**************************************************************************";
cout<<"\nProgrammer :  Sohail Zia Gill.";
cout<<"\nTitle: Display the entered numbers in Ascending order .";
cout<<"\nCompiler : Visual Studio 18";
cout<<"\nDated : 12/Jul/18";
cout<<"\nCompiling date : 12/Jul/18";
cout<<"\nIslamic International University Islamabad.";
cout<<"\n***************************************************************************";
	int a,b,c;
	cout<<"Plz enter three numbers\n";
	cin>>a>>b>>c;
	if(a>b&&b>>c)
		cout<<c<<"\t"<<b<<"\t"<<a;
	else if(b>a&&a>c)
	cout<<c<<"\t"<<a<<"\t"<<b;
	else if(c>b&&b>a)
		cout<<a<<"\t"<<b<<" \t "<<c;
	else if(c>a&&a>b)
		cout<<b<<" \t "<<a<<"\t  "<<c;
		else if(b>c&&c>a)
			cout<<a<<"  \t"<<c<<"\t  "<<b;
		else
			cout<<b<<"  \t"<<c<<"\t  "<<a;
		cout<<endl;
			system("pause");
			return 0;
}
